# App Configuration
APP_NAME=CRAV Logo Studio
APP_BRAND_PRIMARY=#0EA5E9
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase (Database & Auth)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Payments
STRIPE_SECRET_KEY=your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# PayPal Payments (Optional)
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_WEBHOOK_ID=your_paypal_webhook_id

# Redis/Queue (Optional - for background jobs)
REDIS_URL=redis://localhost:6379

# Storage (Optional - for asset uploads)
S3_ENDPOINT=https://your-s3-endpoint
S3_REGION=auto
S3_BUCKET=crav-logo-studio
S3_ACCESS_KEY=your_access_key
S3_SECRET_KEY=your_secret_key

# Analytics & Monitoring (Optional)
POSTHOG_KEY=phc_your_posthog_key
SENTRY_DSN=https://your-sentry-dsn

# Feature Flags (Optional)
FLAG_LOGO_STUDIO=true
FLAG_ANIMATION=true
FLAG_MARKETPLACE=true
FLAG_TEAM_SHARING=true
FLAG_DOMAIN_MAPPING=false

# Embed Configuration (Optional - for CRAV integration)
CRAV_EMBED_ORIGIN=https://app.craudiovizai.com
